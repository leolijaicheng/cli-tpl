{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{description}}",
  "author": "{{ author }}",
  "main": "index.js",
  "scripts": {
    "start": "node build/dev-server.js --mode development",
    "dev": "webpack-dev-server --config build/webpack.dev.js --mode development",
    "build:dll": "webpack build/webpack.dll.js",
    "build:dev": "webpack --config build/webpack.prod.js --mode production",
    "build:ssr": "webpack --config build/webpack.ssr.js --mode production",
    "build": "node build/build.js  --mode production",
    "serve": "node build/serve.js",
    {{#lint}}
    "lint": "eslint --fix --ext .js --ext .vue src",
    {{/lint}}
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "license": "ISC",
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "core-js": "^3.8.2",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "regenerator-runtime": "^0.13.7",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-webpack-plugin": "^2.1.1",
    {{#router}}
    "vue-router": "^3.4.9",
    {{/router}}
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.6.0",
    "vuex-router-sync": "^5.0.0",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "autodll-webpack-plugin": "^0.4.2",
    "autoprefixer": "^8.6.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.3",
    "compression-webpack-plugin": "^6.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^6.4.0",
    "css-loader": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    {{#lint}}
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.4.1",
    "eslint-webpack-plugin": "^2.4.3",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    {{/if_eq}}
    {{/lint}}
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.8",
    "image-webpack-loader": "^7.0.1",
    "less": "^3.12.2",
    "less-loader": "^7.1.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "ora": "^5.1.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "serve-handler": "^6.1.3",
    "style-loader": "^1.3.0",
    "stylelint": "^13.9.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^4.2.3",
    "thread-loader": "^3.0.1",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
